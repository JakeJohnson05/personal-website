{
  "name": "personal-website",
  "version": "0.1.0",
  "author": {
    "name": "Jake Johnson",
    "url": "https://jakejohnson.pro"
  },
  "main": "server.ts",
  "scripts": {
    "prestart": "tsc -p tsconfig.server.json",
    "start": "node ./dist/server.js",
    "express": "nodemon",
    "angular": "ng serve --port 5000 --proxy-config proxy.config.json",
    "build": "ng build --prod",
    "build:dev": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "nodemonConfig": {
    "ignore": [
      "src",
      ".git",
      "node_modules"
    ],
    "watch": [
      "api",
      "server.ts",
      "database.ts"
    ],
    "ext": "ts",
    "exec": "npm start"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.0",
    "@angular/common": "~9.0.0",
    "@angular/compiler": "~9.0.0",
    "@angular/core": "~9.0.0",
    "@angular/forms": "~9.0.0",
    "@angular/platform-browser": "~9.0.0",
    "@angular/platform-browser-dynamic": "~9.0.0",
    "@angular/router": "~9.0.0",
    "@codexist/status-reports": "^3.2.0",
    "@types/node": "^13.7.0",
    "body-parser": "^1.19.0",
    "connect-session-sequelize": "^6.1.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "express-validator": "^6.4.0",
    "helmet": "^3.21.2",
    "mysql2": "^2.1.0",
    "ngx-device-detector": "^1.3.20",
    "nodemailer": "^6.4.2",
    "path": "^0.12.7",
    "rxjs": "^6.5.4",
    "sequelize": "^5.21.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.1",
    "@angular/cli": "^9.0.1",
    "@angular/compiler-cli": "~9.0.0",
    "@angular/language-service": "~9.0.0",
    "@types/express": "^4.17.2",
    "@types/express-session": "^1.15.16",
    "@types/helmet": "0.0.45",
    "@types/jasmine": "^3.5.3",
    "@types/jasminewd2": "~2.0.3",
    "@types/nodemailer": "^6.4.0",
    "codelyzer": "^5.1.2",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "^3.1.0",
    "karma-jasmine-html-reporter": "^1.5.2",
    "nodemon": "^2.0.2",
    "protractor": "^5.4.3",
    "ts-node": "^8.6.2",
    "tslint": "^6.0.0",
    "typescript": "~3.7.5"
  },
  "optionalDependencies": {
    "fsevents": "^2.1.2"
  }
}
